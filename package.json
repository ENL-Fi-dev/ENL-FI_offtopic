{
  "name": "telegram-groups-back",
  "version": "4.0.0",
  "description": "ENL Fi Offtopic Telegram-link tool",
  "main": "Server.js",
  "scripts": {
    "start": "NODE_ENV=production node Server.js",
    "watch": "NODE_ENV=development nodemon Server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedFoxFinn/ENL-FI_offtopic.git"
  },
  "bugs": {
    "url": "https://github.com/RedFoxFinn/ENL-FI_offtopic/issues"
  },
  "author": "RedFoxFinn",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^8.5.1",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.1"
  },
  "proxy": "http://localhost:4005/",
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text",
