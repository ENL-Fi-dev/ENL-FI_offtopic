    "bson-ext": "^2.0.3",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "jsonwebtoken": "^8.5.0",
    "mongodb": "^3.5.5",
    "mongoose": "^5.9.4",
    "morgan": "~1.9.1",
